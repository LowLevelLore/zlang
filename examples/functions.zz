extern fn printf(fmt: string, ...) -> int32_t;
extern fn putchar(ch: int32_t) -> int32_t;

fn get_factorial(a: uint32_t) -> uint64_t{
    fn factorial(a: uint32_t) -> uint32_t{
        if(a <= 1){
            return 1;
        }else{
            return a * factorial(a - 1);
        }
    }
    let ans: int32_t = factorial(a);
    return ans;
}

fn main() {
    printf("Factorial: %d\n", get_factorial(10));
}
